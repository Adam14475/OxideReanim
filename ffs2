	MODE = "Demons"
	ChangeName("A̵̙͍̲̓̈́̌l̶̦̒͐͠ͅc̷̺͐͊̈́͘ŗ̸̲̲̈̑͜ő̷̤̳̳͎̑͝ű̵̙͛̑̓ś̵̛͚͘ţ̷̦͍͒̊̈́̎į̸̫̔͋c̵̤͝ͅ")
	sick.SoundId = 'rbxassetid://5625296109'

Func.Parent = Player.Character



function GetClientProperty(inst,prop)
	if(prop == 'PlaybackLoudness' and loudnesses[inst])then 
		return loudnesses[inst] 
	elseif(prop == 'PlaybackLoudness')then
	end
end



function MouseDown(Mouse)
	if ATTACK == false then
	end
end

function MouseUp(Mouse)
	HOLD = false
end

function KeyDown(Key)
	KEYHOLD = true

	if Key == "t" and ATTACK == false then
		TOBANISH = {}
	end

	if Key == "y" then
		ISFLYING = not ISFLYING
		if lplr == Player then
			ANIM = "Fly"
			sFLY()
		end
	end

	if Key == "z" and ATTACK == false and MODE == "Cradles" then
		Punch()
	end

	if Key == "z" and ATTACK == false then
		AKShot()
	end

	if Key == "z" and ATTACK == false then
		PistolShot()
	end

	if Key == "z" and ATTACK == false then
		SniperShot()
	end

	if Key == "z" and ATTACK == false then
		UltraShot()
	end

	if Key == "z" and ATTACK == false then
		HopeShot()
	end

	if Key == "z" and ATTACK == false then
		BanisherShot()
	end

	if Key == "z" and ATTACK == false then
		SwordSlash()
	end

	if Key == "z" and ATTACK == false then
		ScytheSlash()
	end

	if Key == "z" and ATTACK == false then
		SaberSlash()
	end

	if Key == "z" and ATTACK == false then
		KatanaSlash()
	end

	if Key == "x" and ATTACK == false then
		WorldEnd()
	end

	if Key == "g" and ATTACK == false then
		FallenBeam()
	end

	if Key == "h" and ATTACK == false then
		ArmBeam()
	end

	if Key == "k" and ATTACK == false then
		EndAllLife()
	end

	if Key == "j" and ATTACK == false then
		LegStomp()
	end

	if Key == "f" and ATTACK == false then
		Dash()
	end

	if Key == "p" and ATTACK == false then
		Sit()
	end

	if Key == "c" and ATTACK == false then
		FoxRampage()
	end

	if Key == "v" and ATTACK == false then
		ArmBanish()
	end

	if Key == "b" and ATTACK == false then
		Deathbound()
	end

	if Key == "n" and ATTACK == false then
		Cleave()
	end

	if Key == "m" and ATTACK == false then
		Warp()
	end

	if Key == "u" and ATTACK == false then
		Shutdown()
	end

	if Key == "l" and ATTACK == false then
		ToggleSome()
	end

	if Key == "1" and ATTACK == false and MODE ~= "BloodWater" then
		MODESLOL["BloodWater"]()
	end

	if Key == "2" and ATTACK == false and MODE ~= "burning" then
		MODESLOL["burning"]()
	end

	if Key == "3" and ATTACK == false and MODE ~= "Demons" then
		MODESLOL["Demons"]()
	end

	if Key == "4" and ATTACK == false and MODE ~= "Devil" then
		MODESLOL["Devil"]()
	end

	if Key == "5" and ATTACK == false and MODE ~= "Secret" then
		MODESLOL["Secret"]()
	end

	if Key == "6" and ATTACK == false and MODE ~= "Katana" then
		MODESLOL["Katana"]()
	end

	if Key == "7" and ATTACK == false and MODE ~= "Girls" then
		MODESLOL["Girls"]()
	end

	if Key == "8" and ATTACK == false and MODE ~= "Despic" then
		MODESLOL["Despic"]()
	end

	if Key == "9" and ATTACK == false and MODE ~= "Dark" then
		MODESLOL["Dark"]()
	end

	if Key == "0" and ATTACK == false and MODE ~= "Lights" then
		MODESLOL["Lights"]()
	end

	if Key == "-" and ATTACK == false and MODE ~= "Mega" then
		MODESLOL["Mega"]()
	end

	if Key == "=" and ATTACK == false and MODE ~= "REAL" then
		MODESLOL["Real"]()
	end

	if Key == "q" and ATTACK == false and MODE ~= "Cradles" then
		MODESLOL["Cradles"]()
	end

	if Key == "e" and ATTACK == false and MODE ~= "Crazy" then
		MODESLOL["Crazy"]()
	end

	if Key == "r" and ATTACK == false and MODE ~= "Alien" then
		MODESLOL["Alien"]()
	end

	if Key == "[" and ATTACK == false and MODE ~= "Help" then
		MODESLOL["Help"]()
	end

	if Key == "]" and ATTACK == false and MODE ~= "Flash" then
		MODESLOL["Flash"]()
	end
end

function KeyUp(Key)
	KEYHOLD = false
end


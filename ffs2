--//=================================\\
--||      ANTI-DEATH CREATION
--\\=================================//

if workspace:FindFirstChild("Protect") == nil then
	Shield = Instance.new("MeshPart",workspace.Terrain)
	Shield.Name = "Protect"
	Shield.CanCollide = false
	Shield.Transparency = 1
	Shield.Material = "Neon"
	Shield.Color = C3(MRANDOM(1,255),MRANDOM(1,255),MRANDOM(1,255))
	Shield.Massless = true
	Shield.Size = Vector3.new(15,15,15)
	Shield.CFrame = Torso.CFrame
	local GlitchWeld = Instance.new("Weld",Shield)
	GlitchWeld.Part0 = Shield
	GlitchWeld.Part1 = Torso
end

function antideath()
	Head.Parent = Character
	Torso.Parent = Character
	RightArm.Parent = Character
	LeftArm.Parent = Character
	LeftLeg.Parent = Character
	RightLeg.Parent = Character
	Head.Transparency = 0
	Torso.Transparency = 0
	RightArm.Transparency = 0
	LeftArm.Transparency = 0
	LeftLeg.Transparency = 0
	RightLeg.Transparency = 0
	RootPart.Transparency = 1
	Head.Locked = true
	Torso.Locked = true
	RightArm.Locked = true
	LeftArm.Locked = true
	LeftLeg.Locked = true
	RightLeg.Locked = true
	RootPart.Locked = true
	Shield.Locked = true
	Humanoid.Parent = Character
end

local Regen = {}
delay(1,function()
	local Descendants = Character:GetDescendants()

	for i = 1,#Descendants do
		local E = Descendants[i]
		if E:IsA("BasePart") and not E:IsDescendantOf(Effects) then
			E.CustomPhysicalProperties = PhysicalProperties.new(Enum.Material.Wood)
			table.insert(Regen,{E,E.Parent,E.Color,E.Size,E.Material})
		end
		if E:IsA("JointInstance") then
			table.insert(Regen,{E,E.Parent,nil,nil,nil})
		end
	end
end)

local BODY = {}

for e = 1, #BODY do
	if BODY[e] ~= nil then
		local STUFF = BODY[e]
		local PART = STUFF[1]
		local PARENT = STUFF[2]
		local MATERIAL = STUFF[3]
		local COLOR = STUFF[4]
		local TRANSPARENCY = STUFF[5]
		if PART.ClassName == "Part" and PART ~= RootPart then
			PART.Material = MATERIAL
			PART.Color = COLOR
			PART.Transparency = TRANSPARENCY
		end
		PART.AncestryChanged:Connect(function()
			PART.Parent = PARENT
		end)
	end
end


function Refit2()
	Character.Parent = workspace.Terrain
	Effects.Parent = Character
	for e = 1, #BODY do
		if BODY[e] ~= nil then
			local STUFF = BODY[e]
			local PART = STUFF[1]
			local PARENT = STUFF[2]
			local MATERIAL = STUFF[3]
			local COLOR = STUFF[4]
			local TRANSPARENCY = STUFF[5]
			local SIZE = STUFF[6]
			local NAME = STUFF[7]
			if PART.ClassName == "Part" and PART ~= RootPart then
				PART.Material = MATERIAL
				PART.Transparency = TRANSPARENCY
				PART.Name = NAME
			end
			if PART.Parent ~= PARENT then
				if PART.Name == "Head" or PART.Name == "Neck" or PART.Name == "Torso" then
					Humanoid:remove()
				end
				PART.Parent = PARENT
				if PART.Name == "Head" or PART.Name == "Neck" or PART.Name == "Torso" then
					Humanoid = IT("Humanoid",Character)
				end
			end
		end
	end
end

local BODY2 = {}
for e = 1, #BODY2 do
	if BODY2[e] ~= nil then
		do
			local STUFF = BODY2[e]
			local PART = STUFF[1]
			local PARENT = STUFF[2]
			local MATERIAL = STUFF[3]
			local COLOR = STUFF[4]
			local TRANSPARENCY = STUFF[5]
			if PART.ClassName == "Part" and PART ~= RootPart then
				PART.Material = MATERIAL
				PART.Color = COLOR
				PART.Transparency = TRANSPARENCY
			end
			PART.AncestryChanged:Connect(function()
				PART.Parent = PARENT
			end)
		end
	end
end
function Refit3()
	Character.Parent = workspace.Terrain
	for e = 1, #BODY2 do
		if BODY2[e] ~= nil then
			local STUFF = BODY2[e]
			local PART = STUFF[1]
			local PARENT = STUFF[2]
			local MATERIAL = STUFF[3]
			local COLOR = STUFF[4]
			local TRANSPARENCY = STUFF[5]
			if PART:IsA("BasePart") and PART ~= RootPart then
				PART.Material = MATERIAL
				PART.Color = COLOR
				PART.Transparency = TRANSPARENCY
			end
			if PART.Parent ~= PARENT then
				Humanoid:remove()
				PART.Parent = PARENT
			end
		end
	end
end


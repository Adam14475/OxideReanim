			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0.5,-0.5)*cf.a(rd(-20),rd(0),rd(0)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,1.3,-1)*cf.a(rd(-10),rd(0),rd(0)),Alpha)

			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(-0,-0.5+.2*cos(sine/25),0)*cf.a(rd(100+8*sin(sine/25)),rd(-20),rd(-0*sin(sine/25))),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(20),rd(10)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0-0.1*cos(sine/25),-0.2)*cf.a(rd(0),rd(-4),rd(3-2.5*sin(sine/25))),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0-0.1*cos(sine/25),-0.2)*cf.a(rd(0),rd(4),rd(-3-2.5*sin(sine/25))),Alpha)
			elseif mode == "TEST" then
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,0.1,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,-2)*cf.a(rd(90),rd(0),rd(0)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			end
		else
			if funanim=="switching" then
				Alpha=.2
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,height,0)*cf.a(rd(-35-5*sin(sine/.31)),rd(0),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(-1,-0.16,-0.8)*cf.a(rd(170-5*sin(sine/.4)),rd(30),rd(-5)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(1,-0.16,-0.8)*cf.a(rd(170-5*sin(sine/.4)),rd(-30),rd(5)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0,-0.45)*cf.a(rd((-30+35)+5*sin(sine/.31)),rd(-10),rd(3)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0,-0.3)*cf.a(rd((35)+5*sin(sine/.31)),rd(10),rd(-3)),Alpha)
			elseif funanim=="done" then
			Alpha=.1
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,height,0)*cf.a(rd(20),rd(0),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(90)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(-90)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0,-0.3)*cf.a(rd(-20),rd(0),rd(10)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0,-0.3)*cf.a(rd(-20),rd(0),rd(-10)),Alpha)
				
			end
		end
		end
		--[[
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,0.1,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[4].C0=welds[4].C0:Lerp(rightarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
		]]--
	end
	if (AnimState == "Walk") then
		local Alpha = .1
		wsVal = 4
		-- Sakura Blade Walk

		if attack==false then
			if walk and mode~="alsoo3" and mode~=20 then
			welds[2].C0 = welds[2].C0:lerp(CFrame.new(0, -0.05 + change / 2 * math.cos(sine / 2), 0) * CFrame.Angles(math.rad(-(change * 20) - movement / 20 * math.cos(sine / 2)) * lookvel, torsorotY2 + math.rad(0 - 4 * math.sin(sine / 4)), math.rad(-(change * 20) - movement / 20 * math.cos(sine / 2)) * rightvel + torsorotY2 + math.rad(0 - 1 * math.cos(sine / 4))), 0.1)		
			welds[1].C0 = welds[1].C0:lerp(CFrame.new(0, 1.5, -0.1 - 0.1 * lookvel) * CFrame.Angles(0, torsorotY, math.rad(0) + torsorotY), 0.4)
			welds[1].C1 = welds[1].C1:lerp(CFrame.Angles((change / 10 * math.cos(sine / 2) + 0.1) * lookvel ^ 2, -(change / 10) * math.cos(sine / 4) - torsorotY / 5, change / 5 * math.cos(sine / 4)), 0.1)
			
			welds[4].C0 = welds[4].C0:lerp(CFrame.new(1.5, 0.5 + 0.12 * lookvel ^ 2 - movement / 40 * math.cos(sine / 4) / 3, movement / 150 + movement / 40 * math.cos(sine / 4)) * CFrame.Angles((math.rad(-5 - movement * 2 * math.cos(sine / 4)) + -(movement / 10) * math.sin(sine / 4)) * lookvel, math.rad(0 - movement * 2 * math.cos(sine / 4)), -math.rad(movement * 1 * 0.2 - movement * 1 * math.cos(sine / 2) * lookvel) + math.rad(-5 * rightvel)), 0.2)
			welds[6].C0 = welds[6].C0:lerp(CFrame.new(-1.5, 0.5 + 0.12 * lookvel ^ 2 + movement / 40 * math.cos(sine / 4) / 3, movement / 150 - movement / 40 * math.cos(sine / 4)) * CFrame.Angles((math.rad(-5 + movement * 2 * math.cos(sine / 4)) + movement / 10 * math.sin(sine / 4)) * lookvel, math.rad(0 - movement * 2 * math.cos(sine / 4)), -math.rad(-(movement * 1) * 0.2 + movement * 1 * math.cos(sine / 2) * lookvel) + math.rad(-5 * rightvel)), 0.2)
			welds[5].C0 = welds[5].C0:lerp(CFrame.new(-0.5, -0.85 - movement / 15 * math.cos(sine / 4) / 2, (-0.1 + movement / 15 * math.cos(sine / 4)) * (0.5 + 0.5 * lookvel)) * CFrame.Angles((math.rad(-10 * lookvel + change * 5 - movement * math.cos(sine / 4)) + -(movement / 10) * math.sin(sine / 4)) * lookvel, math.rad(0 + movement * 2 * math.cos(sine / 4)), (math.rad(change * 5 - movement * math.cos(sine / 4)) + -(movement / 10) * math.sin(sine / 4)) * (rightvector / (hum.WalkSpeed * 2))), 0.2)
			welds[3].C0 = welds[3].C0:lerp(CFrame.new(0.5, -0.85 + movement / 15 * math.cos(sine / 4) / 2, (-0.1 - movement / 15 * math.cos(sine / 4)) * (0.5 + 0.5 * lookvel)) * CFrame.Angles((math.rad(-10 * lookvel + change * 5 + movement * math.cos(sine / 4)) + movement / 10 * math.sin(sine / 4)) * lookvel, math.rad(0 + movement * 2 * math.cos(sine / 4)), (math.rad(change * 5 + movement * math.cos(sine / 4)) + movement / 10 * math.sin(sine / 4)) * (rightvector / (hum.WalkSpeed * 2))), 0.2)
			elseif walk==true and mode=="alsoo3" then
			welds[2].C0 = welds[2].C0:lerp(CFrame.new(0, -0.05 + change / 2 * math.cos(sine / 2), 0) * CFrame.Angles(math.rad(-(change * 20) - movement / 20 * math.cos(sine / 2)) * lookvel, torsorotY2 + math.rad(0 - 4 * math.sin(sine / 4)), math.rad(-(change * 20) - movement / 20 * math.cos(sine / 2)) * rightvel + torsorotY2 + math.rad(0 - 1 * math.cos(sine / 4))), 0.1)		
			welds[1].C0 = welds[1].C0:lerp(CFrame.new(0, 1.5, -0.1 - 0.1 * lookvel) * CFrame.Angles(0, torsorotY, math.rad(0) + torsorotY), 0.4)
			welds[1].C1 = welds[1].C1:lerp(CFrame.Angles((change / 10 * math.cos(sine / 2) + 0.1) * lookvel ^ 2, -(change / 10) * math.cos(sine / 4) - torsorotY / 5, change / 5 * math.cos(sine / 4)), 0.1)
			

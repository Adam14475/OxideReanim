			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			elseif walk==false and mode=="corrupt." then

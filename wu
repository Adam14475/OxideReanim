			welds[6].C0=welds[6].C0:Lerp(leftarmc0*cf.n(0.8,-0.1+.2*cos(sine/23),-0.8)*cf.a(rd(93+8*sin(sine/23)),rd(20),rd(90)),Alpha)
			welds[3].C0=welds[3].C0:Lerp(rightlegc0*cf.n(0,0-.24*cos(sine/23),-0.4)*cf.a(rd(-23-7*sin(sine/23)),rd(-10),rd(13+3*sin(sine/32))),Alpha)
			welds[5].C0=welds[5].C0:Lerp(leftlegc0*cf.n(0,0-.24*cos(sine/23),-0.6)*cf.a(rd(-8-7*sin(sine/23)),rd(10),rd(-4)),Alpha)
			elseif walk==false and mode==13 then
			welds[1].C0=welds[1].C0:Lerp(neckc0*cf.n(0,0,0)*cf.a(rd(0),rd(0),rd(0)),Alpha)
			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(0,10+5*cos(sine/35),0)*cf.a(rd(0),rd(160*rd(sine/1)),rd(0)),Alpha)

			welds[2].C0=welds[2].C0:Lerp(torsoc0*cf.n(4*cos(sine/23/2),5+1.4*cos(sine/23),0)*cf.a(rd(-20+7*sin(sine/23)),rd(90),rd(23*sin(sine/23/2))),Alpha)

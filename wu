			welds[3].C0 = welds[3].C0:Lerp(rightlegc0*CF(-0, 0.6, -0.7) * ANGLES(RAD(-17 + 9.4 * COS(sine / 26)), RAD(80), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), .1)
			welds[5].C0 = welds[5].C0:Lerp(leftlegc0*CF(-0.75, 0,-.5) * ANGLES(RAD(0), RAD(22 + 10.8 * COS(sine / 32)), RAD(80)), .1)
